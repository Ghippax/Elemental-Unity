      	image("https://1.bp.blogspot.com/-GzaeQXVx04k/YDFiMFu6ESI/AAAAAAAADv0/nQMg4urJmtA7nY6sq4pfm3V1wO2cY7jmgCLcBGAsYHQ/s0/boltf.png",pos.x,pos.y,width,height);
      break
    }
    if(debug){
      noFill();
      stroke(0);
      rect(pos.x,pos.y,width,height);
    }    
  }
}

