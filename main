  	tileInfo = curStage.getTilesAround(e.pos.x,e.pos.y,e.width,e.height,2+ceil(e.width/TileSize),2+ceil(e.height/TileSize),layer);
  	for(int i = 1; i < sizeOf(tileInfo,2)-1; i++){
