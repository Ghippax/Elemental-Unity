  wind[] aux = arr;
  arr = new wind[aux.length+1];
  for(int i = 0; i < aux.length; i++){
    arr[i] = aux[i];
  }
  arr[aux.length] = new wind(x,y,dir);
  return arr;
}
  
wind[] delWind(wind[] arr, int index){
  wind[] aux = arr;
  arr = new wind[aux.length-1];
  int n;
  for(int i = 0; i < aux.length; i++){
    if(i != index){
    	arr[n] = aux[i]; 
			n++;
    }
  }
  return arr;
}
