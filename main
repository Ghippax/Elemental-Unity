//Convertir de hexadecimal a rgb
int[] fromHexToRGB(String hex){
  int[] c = new int[3];
  char[] nHex = stringToCharArray(hex);
  c[0] = getNum(nHex[0]) * 16 + getNum(nHex[1]);
  c[1] = getNum(nHex[2]) * 16 + getNum(nHex[3]);
  c[2] = getNum(nHex[4]) * 16 + getNum(nHex[5]);   
  return c
}

